<?xml version="1.0"?>
<!DOCTYPE resource-action-mapping PUBLIC "-//Liferay//DTD Resource Action Mapping 7.0.0//EN" "http://www.liferay.com/dtd/liferay-resource-action-mapping_7_0_0.dtd">

<resource-action-mapping>

<model-resource>
    <model-name>com.liferay.famocom.erpcloud.model.TimeSheetEntry</model-name>
    <portlet-ref>
        <portlet-name>MyTimesheet</portlet-name>
    </portlet-ref>
    <permissions>
        <supports>
            <action-key>ADD_ENTRY</action-key>
            <action-key>DELETE</action-key>
            <action-key>UPDATE</action-key>
            <action-key>MANAGER_VIEW</action-key>
			<action-key>HR_VIEW</action-key>
        </supports>
        <site-member-defaults>
            <action-key>ADD_ENTRY</action-key>
           <action-key>MANAGER_VIEW</action-key>
			<action-key>HR_VIEW</action-key>
        </site-member-defaults>
        <guest-defaults>
            <action-key>MANAGER_VIEW</action-key>
			<action-key>HR_VIEW</action-key>
        </guest-defaults>
        <guest-unsupported>
            <action-key>UPDATE</action-key>
        </guest-unsupported>
    </permissions>
</model-resource>
</resource-action-mapping>